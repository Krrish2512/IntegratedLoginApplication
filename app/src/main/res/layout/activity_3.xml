<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="2dp"
    android:background="@drawable/img_16"
    tools:context=".MainActivity">


    <ImageButton
        android:id="@+id/Torch"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="95dp"
        android:layout_marginBottom="39dp"
        android:backgroundTint="#0E4575"
        android:src="@drawable/flashlighttt"
        android:text="Torch"
        app:layout_constraintBottom_toTopOf="@+id/MessShare"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/Calc"
        app:layout_constraintTop_toBottomOf="@+id/Tic" />

    <ImageButton
        android:id="@+id/Paint"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/Torch"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="150dp"
        android:layout_marginTop="150dp"
        android:layout_marginEnd="20dp"
        android:layout_marginBottom="30dp"
        android:backgroundTint="#0E4575"
        android:src="@drawable/paintxx"
        android:text="Scribble"
        app:layout_constraintBottom_toTopOf="@+id/Calc"
        app:layout_constraintEnd_toStartOf="@+id/Tic"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/BMI_app"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/Paint"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="94dp"
        android:layout_marginTop="17dp"
        android:layout_marginEnd="20dp"
        android:layout_marginBottom="25dp"
        android:backgroundTint="#0E4575"
        android:src="@drawable/bmipic"
        android:text="BMI Calculator"
        app:layout_constraintBottom_toTopOf="@+id/Devs"
        app:layout_constraintEnd_toStartOf="@+id/MessShare"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Calc" />

    <ImageButton
        android:id="@+id/MessShare"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/BMI_app"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="20dp"
        android:layout_marginTop="18dp"
        android:layout_marginEnd="97dp"
        android:layout_marginBottom="49dp"
        android:backgroundTint="#0E4575"
        android:src="@drawable/sharexxx"
        android:text="Message Share"
        app:layout_constraintBottom_toTopOf="@+id/Devs"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/BMI_app"
        app:layout_constraintTop_toBottomOf="@+id/Torch" />

    <ImageButton
        android:id="@+id/Calc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/MessShare"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="94dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="20dp"
        android:layout_marginBottom="39dp"
        android:backgroundTint="#0E4575"
        android:src="@drawable/calcxxx"
        android:text="Calculator"
        app:layout_constraintBottom_toTopOf="@+id/BMI_app"
        app:layout_constraintEnd_toStartOf="@+id/Torch"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Paint" />

    <ImageButton
        android:id="@+id/Tic"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="20dp"
        android:layout_marginTop="170dp"
        android:layout_marginEnd="150dp"
        android:layout_marginBottom="30dp"
        android:backgroundTint="#0E4575"
        android:src="@drawable/tttxxx"
        android:text="Game Zone"
        app:layout_constraintBottom_toTopOf="@+id/Torch"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/Paint"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/Devs"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/MessShare"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="165dp"
        android:layout_marginTop="408dp"
        android:layout_marginEnd="160dp"
        android:layout_marginBottom="112dp"
        android:backgroundTint="#0E4575"
        android:src="@drawable/devsexy"
        android:text="Developers"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="84dp"
        android:textSize="30sp"
        android:textColor="#ffffff"
        android:text="ð˜¿ð˜¼ð™Žð™ƒð˜½ð™Šð˜¼ð™ð˜¿"
        app:layout_constraintEnd_toStartOf="@+id/Tic"
        app:layout_constraintStart_toEndOf="@+id/Paint"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>